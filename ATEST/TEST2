import sys,time
def sprint(str):
   for c in str + '\n':
     sys.stdout.write(c)
     sys.stdout.flush()
     time.sleep(1./5)

sprint('HELLO THERE! WELCOME TO YOUR BRAND NEW ASSISTANT!')

